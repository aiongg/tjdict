{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["head", "defs"],
  "additionalProperties": false,
  "properties": {
    "head": {
      "type": "string",
      "minLength": 1
    },
    "head_number": {
      "type": "number",
      "minimum": 1
    },
    "page": {
      "type": "number",
      "minimum": 1
    },
    "etym": {
      "type": "string"
    },
    "defs": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "#/definitions/PosDefinition"
      }
    }
  },
  "definitions": {
    "PosDefinition": {
      "type": "object",
      "required": ["pos", "defs"],
      "additionalProperties": false,
      "properties": {
        "pos": {
          "type": "string"
        },
        "mw": {
          "type": "string"
        },
        "etym": {
          "type": "string"
        },
        "defs": {
          "type": "array",
          "minItems": 1,
          "items": {
            "$ref": "#/definitions/SubDefinition"
          }
        }
      }
    },
    "SubDefinition": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "en": {
          "type": "string"
        },
        "mw": {
          "type": "string"
        },
        "cat": {
          "type": "string"
        },
        "etym": {
          "type": "string"
        },
        "alt": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "cf": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "det": {
          "type": "string"
        },
        "ex": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ExampleItem"
          }
        },
        "drv": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ExampleItem"
          }
        },
        "idm": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ExampleItem"
          }
        }
      }
    },
    "TranslationVariant": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "en": {
          "type": "string"
        },
        "mw": {
          "type": "string"
        },
        "cat": {
          "type": "string"
        },
        "etym": {
          "type": "string"
        },
        "alt": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "ex": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ExampleItem"
          }
        }
      }
    },
    "ExampleItem": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "tw": {
          "type": "string"
        },
        "en": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TranslationVariant"
          }
        },
        "mw": {
          "type": "string"
        },
        "cat": {
          "type": "string"
        },
        "etym": {
          "type": "string"
        },
        "det": {
          "type": "string"
        },
        "alt": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "cf": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "ex": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ExampleItem"
          }
        },
        "drv": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ExampleItem"
          }
        },
        "idm": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ExampleItem"
          }
        }
      }
    }
  }
}

